{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:vue/essential",
        "google","eslint:recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-use-before-define": ["error", { "variables": false }],
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "object-curly-spacing": [2, "always"],
       "indent":"off",
       "max-len":"off"
          
      }
    
}